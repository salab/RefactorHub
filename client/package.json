{
  "name": "refactorhub",
  "version": "1.0.0",
  "description": "Dataset creation support tool for empirical studies on refactoring activity.",
  "author": "Ryo Kuramoto <kuramoto@se.cs.titech.ac.jp>",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "generate:with-api": "run-s gen-api generate",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier **/*.{ts,vue,scss} --check --ignore-path .gitignore",
    "lint:stylelint": "stylelint **/*.{vue,scss} --ignore-path .gitignore",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "clear": "rimraf src/apis/generated",
    "gen-api": "openapi-generator-cli generate -g typescript-axios -i ../docs/swagger.yml -o ./src/apis/generated"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/axios": "^5.13.0",
    "@nuxtjs/composition-api": "^0.19.0",
    "@types/lodash-es": "^4.17.4",
    "axios": "^0.21.1",
    "change-case": "^4.1.2",
    "consola": "^2.15.0",
    "core-js": "^3.8.3",
    "crypto-random-string": "^3.3.0",
    "lodash-es": "^4.17.20",
    "monaco-editor": "^0.22.3",
    "nuxt": "^2.14.12",
    "nuxt-typed-vuex": "^0.1.22",
    "object-hash": "^2.1.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.11.3",
    "@openapitools/openapi-generator-cli": "^2.1.20",
    "@types/object-hash": "^1.3.4",
    "deepmerge": "^4.2.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "monaco-editor-webpack-plugin": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "nuxt-webfontloader": "^1.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.1.3"
  }
}
