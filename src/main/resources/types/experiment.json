[
  {
    "name": "Extract Method",
    "description": "メソッドの一部を切り出して，別のメソッドにする",
    "url": "https://refactoring.com/catalog/extractFunction.html",
    "before": {
      "extracted code": {
        "description": "抽出されるコード片",
        "type": "CodeFragment",
        "multiple": true,
        "required": true
      },
      "target method": {
        "description": "抽出が行われるメソッド",
        "type": "MethodDeclaration",
        "autofills": [
          {
            "type": "Surround",
            "follows": [
              {
                "category": "before",
                "key": "extracted code"
              }
            ],
            "element": "MethodDeclaration"
          }
        ]
      }
    },
    "after": {
      "invocation": {
        "description": "メソッド呼び出しに置き換わった箇所",
        "type": "MethodInvocation",
        "required": true
      },
      "extracted code": {
        "description": "抽出されたコード片",
        "type": "CodeFragment",
        "multiple": true,
        "required": true
      },
      "extracted method": {
        "description": "抽出して新たに定義されたメソッド",
        "type": "MethodDeclaration",
        "autofills": [
          {
            "type": "Surround",
            "follows": [
              {
                "category": "after",
                "key": "extracted code"
              }
            ],
            "element": "MethodDeclaration"
          }
        ]
      },
      "target method": {
        "description": "抽出が行われたメソッド",
        "type": "MethodDeclaration",
        "autofills": [
          {
            "type": "Surround",
            "follows": [
              {
                "category": "after",
                "key": "invocation"
              }
            ],
            "element": "MethodDeclaration"
          }
        ]
      }
    }
  },
  {
